concept ModelElement
{
    location: SourceLocation;
    scope: Scope;
}

concept NamedElement: ModelElement
{
    name: String;
}

concept Model: NamedElement
{
    concepts: Concept*;
    targets: Target*;
}

concept Concept: NamedElement;

concept Target: NamedElement
{
    properties: Property*;
}

concept Property: NamedElement
{
    value: String;
}
