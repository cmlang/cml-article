operationDecl(name, params, resultType, block) ::= <<
<resultType> <name>(<params:paramDecl(); separator=", ">)
{
    <block>
}
>>

paramDecl(param) ::= <<
<fieldTypeDecl(param)> <fieldName(param)>
>>

fieldTypeDecl_optional(type) ::= <<
@Nullable <typeName(type)>
>>

fieldTypeDecl_set(type) ::= <<
Set\<<typeName(type)>\>
>>
